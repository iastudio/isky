- var active = 2
- var pageName = 'Типовая внутренняя страница'
include includes/settings
doctype html
html(lang='ru').no-js
  head
    include includes/head
  body
    .page-wrap
      include includes/header
      main
        .container
          aside
            h2 Каталог продукции
            nav.accordion
              ul
                li
                  a(href="#").accordion__parent Автопринадлежности
